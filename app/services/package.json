  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "_moduleAliases": {
    "~constants": "dist/constants",
    "@api": "dist/src/api",
    "@config": "dist/src/config",
    "@errors": "dist/src/errors",
    "@logging": "dist/src/logging",
    "@middlewares": "dist/src/middlewares",
    "@models": "dist/src/models",
    "@routes": "dist/src/routes",
    "@services": "dist/src/services",
    "@utils": "dist/src/utils"
  },
  "scripts": {
    "prepare": "cd .. && if [ -f \"./package.json\" ] && [ -d './.husky' ]; then npm install; fi",
    "dev": "node --loader ts-node/esm -r dotenv/config index.ts dotenv_config_path=./.env.development.local | npx pino-pretty",
    "build": "rm -rf dist && tsc",
    "serve": "npm run build && node -r module-alias/register dist/index.js | npx pino-pretty",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.1",
    "@types/cors": "^2.8.14",
