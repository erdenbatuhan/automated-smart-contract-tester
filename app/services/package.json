    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "module-alias": "^2.2.3",
    "mongoose": "^7.4.3",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.15.0"
  }
