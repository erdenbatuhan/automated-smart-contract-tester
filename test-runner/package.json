  "repository": {
    "type": "git",
    "url": "git+https://github.com/erdenbatuhan/automated-smart-contract-testing-app.git"
  },
  "bugs": {
    "url": "https://github.com/erdenbatuhan/automated-smart-contract-testing-app/issues"
  },
  "homepage": "https://github.com/erdenbatuhan/automated-smart-contract-testing-app#readme",
  "_moduleAliases": {
    "@Constants": "dist/src/Constants",
    "@Logger": "dist/src/Logger",
    "@errors": "dist/src/errors",
    "@forge": "dist/src/forge",
    "@middlewares": "dist/src/middlewares",
    "@models": "dist/src/models",
    "@rabbitmq": "dist/src/rabbitmq",
    "@rest": "dist/src/rest",
    "@services": "dist/src/services",
    "@utils": "dist/src/utils"
  },
  "scripts": {
    "prepare": "cd .. && if [ -f \"./package.json\" ] && [ -d './.husky' ]; then npm install; fi",
    "dev": "node --loader ts-node/esm -r dotenv/config index.ts dotenv_config_path=./.env.development.local | npx pino-pretty",
    "build": "rm -rf dist && tsc && cp -r templates dist/templates",
    "serve": "npm run build && node -r module-alias/register dist/index.js",
    "start": "if [ \"$ENV\" = \"dev\" ]; then npm run dev; else npm run serve; fi",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/pino": "^7.0.5",
    "adm-zip": "^0.5.10",
    "amqplib": "^0.10.3",
    "axios": "^1.5.0",
