    "dockerode": "^3.3.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "memory-streams": "^0.1.3",
    "module-alias": "^2.2.3",
    "mongodb": "^6.0.0",
    "mongoose": "^7.4.3",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.15.0",
    "tar": "^6.1.15"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.1",
