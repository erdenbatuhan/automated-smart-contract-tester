    "@types/multer": "^1.4.7",
    "@types/tar": "^6.1.5",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
